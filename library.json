{
    "name": "uniot-cbor",
    "keywords": "uniot, cbor, binary format, iot",
    "description": "A constrained node implementation of CBOR in C (Uniot modification)",
    "repository": {
        "type": "git",
        "url": "https://github.com/uniot-io/uniot-cbor.git"
    },
    "version": "0.2.4",
    "license": "MIT",
    "build": {
        "includeDir": ".",
        "srcFilter": "+<*> -<.git/> -<.svn/> -<example/> -<examples/> -<test/> -<tests/> -<cmake/> -<.editorconfig> -<.gitignore> -<.travis.yml> -<*CMakeLists.txt>"
    },
    "frameworks": "arduino",
    "platforms": [
        "espressif8266",
        "espressif32"
    ]
}
